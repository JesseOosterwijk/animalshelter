<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="684.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.PrimaryController">
   <children>
      <Button fx:id="addAnimalBtn" layoutX="50.0" layoutY="154.0" mnemonicParsing="false" onAction="#addAnimal" text="Add Animal" />
      <TextField fx:id="animalName" layoutX="15.0" layoutY="85.0" />
      <ComboBox fx:id="animalType" layoutX="14.0" layoutY="14.0" onAction="#SwitchType" prefWidth="150.0" />
      <Button fx:id="reserveAnimalBtn" layoutX="41.0" layoutY="239.0" mnemonicParsing="false" onAction="#reserveAnimal" text="Reserve Animal" />
      <ComboBox fx:id="animalGender" layoutX="14.0" layoutY="49.0" prefWidth="150.0" />
      <TextField fx:id="badHabits" layoutX="15.0" layoutY="120.0" />
      <ListView fx:id="animalLv" layoutX="189.0" layoutY="14.0" prefHeight="370.0" prefWidth="483.0" />
      <TextField fx:id="reservorName" layoutX="15.0" layoutY="199.0" />
      <Button fx:id="saveFileBtn" layoutX="47.0" layoutY="352.0" mnemonicParsing="false" onAction="#Save" text="Save Animals" />
   </children>
</AnchorPane>
